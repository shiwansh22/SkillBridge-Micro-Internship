{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/dist/index.html" }
  ],
  "buildCommand": "chmod +x node_modules/.bin/vite && npm install && npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_BASE_URL": "https://skillbridge-micro-internship-production.up.railway.app"
  }
}
